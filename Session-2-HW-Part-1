x = ['mix', 'xyz', 'apple', 'xanadu', 'rovio']

# create two lists to sort in 
xstrings = []
strings = []

# Categorize the words based on whether they start with a x
for string in x:
    if string.startswith('x'):
        xstrings.append(string)
    else:
        strings.append(string)

# Sort the two lists
xstrings.sort()
strings.sort()

# Make the final list in order with the x words first
sortedstrings = xstrings + strings

print(sortedstrings)
